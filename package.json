{
  "name": "runcheck",
  "description": "A tiny (less than 2 KiB Gzipped) and treeshakable! lib for typescript runtime type checks with autofix support",
  "version": "0.21.0",
  "license": "MIT",
  "scripts": {
    "test": "vitest --ui",
    "test:run": "vitest run",
    "lint": "pnpm tsc && pnpm eslint",
    "tsc": "tsc -p tsconfig.prod.json",
    "vitest-bench": "vitest bench",
    "benchmark": "tsm benchmarks/benchmark.ts",
    "benchmark:generate-profile": "tsm benchmarks/generateProfiles.ts",
    "eslint": "CI=true eslint --color --ext .jsx,.js,.ts,.tsx src/ tests/",
    "build": "pnpm test:run && pnpm lint && pnpm build:no-test",
    "build:no-test": "tsup --minify --dts",
    "build-test": "tsup --config tsup.test.config.ts",
    "npm-publish": "./scripts/check-if-is-sync.sh && pnpm build && npm publish"
  },
  "files": [
    "dist"
  ],
  "packageManager": "^pnpm@7.6.0",
  "repository": "github:lucasols/runcheck",
  "author": "Lucas Santos",
  "type": "module",
  "main": "./dist/runcheck.cjs",
  "module": "./dist/runcheck.js",
  "types": "./dist/runcheck.d.ts",
  "exports": {
    ".": {
      "import": "./dist/runcheck.js",
      "require": "./dist/runcheck.cjs"
    },
    "./autofixable": {
      "import": "./dist/autofixable.js",
      "require": "./dist/autofixable.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "autofixable": [
        "./dist/autofixable.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=14.6.0"
  },
  "devDependencies": {
    "@lucasols/eslint-plugin-extended-lint": "^0.1.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitest/ui": "^0.27.1",
    "eslint": "^8.31.0",
    "eslint-plugin-vitest": "^0.0.29",
    "myzod": "^1.8.8",
    "tsm": "^2.3.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "v8-profiler-next": "^1.9.0",
    "vite": "^4.0.4",
    "vitest": "^0.27.1",
    "zod": "^3.20.2"
  }
}
